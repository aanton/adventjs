{
  "env": {
    "node": true,
    "es2020": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "eqeqeq": "error",
    "no-console": "error",
    "no-var": "error",
    "object-shorthand": "error",
    "prefer-const": "error"
  }
}
